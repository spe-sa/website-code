<head>
   <link href="css/yamm.css" rel="stylesheet">
</head>

<nav class="yamm navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="{{ link }}">{{ branding }}</a>
    </div>
    <ul class="nav navbar-nav">
        {% for item in items %}
            {% if item.is_back_up %}
                </div></ul></li>
            {% endif %}
            {% if item.is_divider %}
                <li role="separator" class="divider"></li>
            {% else %}
                {%  if item.is_dropdown_node %}
                    <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">{{ item.title }}
                        <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                        <div class="row-fluid">
                {% else %}
                    {% if item.is_dropdown_header %}
                        <li class="span4" class="dropdown-header">{{ item.title }}</li>
                    {% else %}
                        <li class="span4"><a href="{{ item.get_absolute_url }}">{{ item.title }}</a></li>
                    {% endif %}
                {% endif %}
                {% if forloop.last %}
                    {% if item.level == 2 %}
                        </ul></li>
                    {% endif %}
                {% endif %}
            {% endif %}
        {%  endfor %}
    </ul>
  </div>
</nav>